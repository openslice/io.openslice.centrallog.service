FROM adoptopenjdk/openjdk13-openj9:latest
MAINTAINER openslice.io
RUN mkdir /opt/shareclasses
RUN mkdir -p /opt/openslice/lib/
COPY target/io.openslice.centrallog.service-0.0.1-SNAPSHOT.jar /opt/openslice/lib/
CMD ["java", "-Xshareclasses:cacheDir=/opt/shareclasses", "-jar", "/opt/openslice/lib/io.openslice.centrallog.service-0.0.1-SNAPSHOT.jar"]